meta {
  name: create token
  type: http
  seq: 1
}

post {
  url: {{rootUrl}}/token/create
  body: json
  auth: inherit
}

body:json {
  {
    "tokenName": "TokenTest",
    "symbol": "TKA",
    "tokenValue": 100
  }
}

script:post-response {
  if (res.status === 201) {
    bru.setEnvVar("operatorTokenId", res.body.tokenCreateRx.tokenId);
  }
  
}
